@typeparam T

<table class="table">
    <thead>
        <tr>
            @Header
        </tr>
    </thead>
    <tbody>
        @foreach (T v in Collection)
        {
            <tr>
                @Body(v)
            </tr>
            
        }
    </tbody>
</table>


@code {
    [Parameter]
    public IEnumerable<T> Collection { get; set; } = null!;
    [Parameter]
    public RenderFragment Header { get; set; } = null!;
    [Parameter]
    public RenderFragment<T> Body { get; set; } = null!;
}
